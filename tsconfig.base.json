{
	"compilerOptions": {
		"importHelpers": true,
		"isolatedModules": true,
		"lib": ["dom", "es2022"],
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"noEmitOnError": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2022",
		"customConditions": ["@multi-workspace/source"]
	}
}
