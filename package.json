{
	"name": "@multi-workspace/source",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"lint": "bunx biome check apps/frontend-nextjs apps/frontend-angular1 apps/frontend-angular2 tailwind.config.js",
		"format": "bunx biome check --write apps/frontend-nextjs apps/frontend-angular1 apps/frontend-angular2 tailwind.config.js",
		"dev:backend": "NX_ISOLATE_PLUGINS=false bunx nx serve backend-dotnet",
		"dev:next": "NX_ISOLATE_PLUGINS=false bunx nx dev frontend-nextjs",
		"dev:angular:user": "NX_ISOLATE_PLUGINS=false bunx nx serve frontend-angular1",
		"dev:angular:admin": "NX_ISOLATE_PLUGINS=false bunx nx serve frontend-angular2 --port=4300",
		"dev:all": "concurrently \"bun run dev:backend\" \"bun run dev:next\" \"bun run dev:angular:user\" \"bun run dev:angular:admin\""
	},
	"private": true,
	"dependencies": {
		"@angular/common": "~20.3.0",
		"@angular/compiler": "~20.3.0",
		"@angular/core": "~20.3.0",
		"@angular/forms": "~20.3.0",
		"@angular/platform-browser": "~20.3.0",
		"@angular/platform-browser-dynamic": "~20.3.0",
		"@angular/router": "~20.3.0",
		"next": "~15.2.4",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"rxjs": "~7.8.0",
		"zone.js": "~0.15.0"
	},
	"devDependencies": {
		"@angular-devkit/core": "~20.3.0",
		"@angular-devkit/schematics": "~20.3.0",
		"@angular/build": "~20.3.0",
		"@angular/cli": "~20.3.0",
		"@angular/compiler-cli": "~20.3.0",
		"@angular/language-service": "~20.3.0",
		"@biomejs/biome": "^1.9.3",
		"@nx/angular": "^22.0.1",
		"@nx/js": "22.0.1",
		"@nx/next": "^22.0.1",
		"@nx/web": "22.0.1",
		"@nx/workspace": "^22.0.1",
		"@schematics/angular": "~20.3.0",
		"@swc-node/register": "~1.9.1",
		"@swc/cli": "~0.6.0",
		"@swc/core": "~1.5.7",
		"@swc/helpers": "~0.5.11",
		"@types/node": "20.19.9",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"autoprefixer": "10.4.13",
		"concurrently": "^9.2.1",
		"nx": "22.0.1",
		"postcss": "8.4.38",
		"tailwindcss": "3.4.3",
		"ts-node": "10.9.1",
		"tslib": "^2.3.0",
		"typescript": "~5.9.2"
	},
	"workspaces": ["apps/*"]
}
